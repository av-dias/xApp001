{
  "expo": {
    "name": "Kluor",
    "slug": "Kluor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/kluor-icon.png",
    "scheme": "kluor",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kluor-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.avdias.Kluor"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/kluor-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/kluor-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c27c7e0d-faea-47aa-87bc-c6c94715a556"
      }
    }
  }
}
